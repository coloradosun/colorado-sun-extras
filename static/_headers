# Prevent caching of page-data and app-data
/page-data/*
  Cache-Control: public, max-age=0, must-revalidate

/app-data.json
  Cache-Control: public, max-age=0, must-revalidate

# Cache static assets with content hash in filename
/*.js
  Cache-Control: public, max-age=31536000, immutable

/*.css
  Cache-Control: public, max-age=31536000, immutable
